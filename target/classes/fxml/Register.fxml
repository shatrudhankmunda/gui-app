<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="354.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gui.app.controller.RegisterController">
    <children>
        <Label layoutX="131.0" layoutY="30.0" style="-fx-font-size: 24px;" text="Register" />
        <TextField fx:id="usernameField" layoutX="100.0" layoutY="82.0" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="100" layoutY="130" promptText="Password" />
        <PasswordField fx:id="confirmPasswordField" layoutX="100" layoutY="180" promptText="Confirm Password" />
        <Button layoutX="101.0" layoutY="275.0" onAction="#handleRegister" text="Register" />
        <Hyperlink layoutX="100.0" layoutY="308.0" onAction="#goToLogin" text="Back to Login" />
    </children>
</AnchorPane>
