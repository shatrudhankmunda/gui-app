<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="354.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gui.app.controller.LoginController">
    <children>
        <Label layoutX="136.0" layoutY="29.0" style="-fx-font-size: 24px;" text="Login" />
        <TextField fx:id="usernameField" layoutX="100" layoutY="80" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="100" layoutY="130" promptText="Password" />
        <Button layoutX="100" layoutY="180" onAction="#handleLogin" text="Login" />
        <Hyperlink layoutX="100" layoutY="220" onAction="#goToRegister" text="Register" />
    </children>
   <opaqueInsets>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
   </opaqueInsets>
</AnchorPane>
